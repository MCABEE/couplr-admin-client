{
  "name": "couplr-admin-client",
  "version": "0.1.0",
  "private": true,
"scripts": {
  "dev": "env-cmd -f env/.env.local next dev -p 3000",
  "build": "next build",
  "start": "next start -p 3000",
  "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
  
  "build:local": "env-cmd -f env/.env.local next build",
  "start:local": "env-cmd -f env/.env.local next start -p 3000",

  "build:dev": "env-cmd -f env/.env.dev next build",
  "start:dev": "env-cmd -f env/.env.dev next start -p 3001",

  "build:staging": "env-cmd -f env/.env.staging next build",
  "start:staging": "env-cmd -f env/.env.staging next start -p 3002",

  "build:prod": "env-cmd -f env/.env.prod next build",
  "start:prod": "env-cmd -f env/.env.prod next start -p 3003"
},

  "dependencies": {
    "axios": "^1.8.4",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "env-cmd": "^10.1.0",
    "next": "^15.2.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "webpack": "^5.98.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4"
  }
}
